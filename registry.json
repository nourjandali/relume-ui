{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "relume",
  "homepage": "https://relume-ui-beta.vercel.app/",
  "items": [
    {
      "name": "relume",
      "type": "registry:block",
      "description": "A collection of Relume UI components, blocks, and styleguide",
      "dependencies": [
        "@radix-ui/react-slot",
        "class-variance-authority",
        "framer-motion",
        "react",
        "react-icons"
      ],
      "files": [
        {
          "type": "registry:component",
          "path": "components/navbar6.jsx"
        },
        {
          "type": "registry:component",
          "path": "components/footer9.jsx"
        },
        {
          "type": "registry:component",
          "path": "components/cta3.jsx"
        },
        {
          "type": "registry:ui",
          "path": "ui/button.tsx"
        },
        {
          "type": "registry:ui",
          "path": "ui/card.tsx"
        },
        {
          "type": "registry:ui",
          "path": "ui/input.tsx"
        },
        {
          "type": "registry:hook",
          "path": "hooks/use-media-query.ts"
        },
        {
          "type": "registry:lib",
          "path": "lib/utils.ts"
        },
        {
          "type": "registry:file",
          "path": "globals.css",
          "target": "globals.css"
        }
      ]
    }
  ]
}